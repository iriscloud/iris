<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" name="iris" packages="">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{MMdd HH:mm:ss,SSS}:%c{1}:%L %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>

    </Loggers>
</Configuration>